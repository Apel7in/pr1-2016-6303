#include <stdio.h>
#include<string.h>
#include<stdlib.h>

int main() {
  char* ptr1 = malloc(100*sizeof(char));
  char* ptr2 = malloc(100*sizeof(char));
    scanf("%s", ptr1);
    int i;
    int j=0;
    for (i=0; i<strlen(ptr1); ++i) {
        if ( (i%2)==1) {
            ptr2[j]=ptr1[i];
            j++;
        }
    }
    printf("%s\n", ptr2);
    return 0;
}
