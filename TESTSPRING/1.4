#include <stdio.h>
#include <string.h>
int main()
{   
  char str[1000]; 
  int c;   
  int result=0; 
  scanf ("%c %s", &c,&str); 
  int i;   
  for (i=0; i<=strlen(str); ++i  ) 
  {     
    if (str[i]=c)    
    {        
    result++;    
    }       
  }   
  printf("%d", result);   
  return 0;
  }
